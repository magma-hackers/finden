<div class='container'>
  <div class='row'>
    <div class='col s12 m6 l12 '> 
      <p id="notice"><%= notice %></p>
        <% if current_user.role == 'player' %>
            <h1><%= "Mis visorias" %></h1>
            <table>
                <thead>
                <tr>
                    <th><%= t('visoria.date') %></th>
                    <th><%= t('visoria.place') %></th>
                    <th><%= t('visoria.schedule') %></th>
                    <th><%= t('visoria.club') %></th>
                    <th><%= t('visoria.description') %></th>
                    <th><%= t('visoria.category') %></th>
                    <th colspan="3"></th>
                </tr>
                </thead>
                <tbody>
                <% @my_visorium.each do |visorium| %>
                    <tr>
                    <td><%= visorium.date %></td>
                    <td><%= visorium.place %></td>
                    <td><%= visorium.schedule %></td>
                    <td><%= visorium.club %></td>
                    <td><%= visorium.description %></td>
                    <td><%= visorium.category %></td>
                    <% if can? :read, Visorium %>
                        <td><%= link_to t('visoria.not_assist'), visorium_destroy_assist_path(visorium.id), data: { confirm: t('visoria.alert_message') } %></td>
                    <% end %>
                <% end %>
                </tbody>
            </table>
        <% end %>
    </div>
  </div>
</div>



