 <div class='container'>
    <div class='row'>
        <div class='col s12 m6 l12 '> 
		<h1>Asistencia a Visor√≠a</h1>
		<table>
			<thead>
				<tr>
					<th><%= t('personal_info.name') %></th>
					<th><%= t('personal_info.birth_place') %></th>
					<th><%= t('personal_info.born_date') %></th>
					<th><%= t('personal_info.weight') %></th>
					<th><%= t('personal_info.height') %></th>
					<th><%= t('personal_info.gender') %></th>
					<th colspan="3"></th>
				</tr>
			</thead>

			<tbody>
				<% @list_assist.each do |list_assist| 
					personal_info = list_assist.information_personals %>
					<tr>
						<td><%= personal_info[0].name %></td>
						<td><%= personal_info[0].birth_place %></td>
						<td><%= personal_info[0].born_date %></td>
						<td><%= personal_info[0].weight %></td>
						<td><%= personal_info[0].height %></td>
						<td><%= personal_info[0].gender %></td>
					</tr>
				<% end %>
			</tbody>
		</table>
		<br /><%= link_to 'Imprimir pdf', visorium_print_list_pdf_path(format: 'pdf'), class: "btn" %>
        </div>
    </div>
</div>